{
    "version": 2,
    "builds": [
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "comcast-portal/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/back/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/comcast-portal/build/$1"
      }
    ]
  }
  