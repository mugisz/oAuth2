{
    "version": 2,
    "builds": [
      {
        "src": "server/index.js",
        "use": "@vercel/node",
        "config": {
          "maxLambdaMemory": 1024,
          "maxLambdaDuration": 10
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server/index.js"
      }
    ]
  }